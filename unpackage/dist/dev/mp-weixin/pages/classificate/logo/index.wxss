.mine {
  height: 100%;
  width: 100%;
}
.mine ._header {
  height: 400rpx;
  background-image: url(/static/images/mine.jpg);
  background-size: 100% 100%;
  color: #fff;
}
.mine ._header .kongbai {
  height: 32%;
}
.mine ._header .iconfont {
  font-size: 65px;
}
.mine ._header .zhiliao {
  width: 100%;
  display: flex;
}
.mine ._header .zhiliao .bjzl {
  font-size: 12px;
}
.mine ._section {
  height: 8%;
  background: #fff;
  margin-top: -10px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}
.mine ._section .iconfont {
  font-size: 30px;
}
.mine ._footer {
  height: auto;
  margin-top: 2%;
  background-color: #fff;
}

